<div class="contents row">
    <%= form_for @tweet do |f| %>
      <h3>
        編集する
      </h3>
      <%= f.text_field :image, placeholder: "Image Url" %>
      <%= f.text_field :text, placeholder: "text" %>
      <%= f.text_area :place, placeholder: "場所" %>
      <%= f.submit "SENT" %>
    <% end %>
</div>