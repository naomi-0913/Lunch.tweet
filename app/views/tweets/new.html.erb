<div class="contents row">
 <div class ="container">
    <%= form_for @tweet do |f| %>
    <h3>
      投稿する
    </h3>
    <%= f.text_field :image, placeholder: "Image Url" %>
    <%= f.text_area :text, placeholder: "店名、感想など" %>
    <%= f.text_area :place, placeholder: "場所" %>
    <%= f.submit "SENT" %>
  <% end %>
 </div>
</div>